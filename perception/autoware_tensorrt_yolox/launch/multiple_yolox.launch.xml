  <include if="$(eval &quot;'$(var image_number)'>='1'&quot;)" file="$(find-pkg-share autoware_tensorrt_yolox)/launch/yolox.launch.xml">
    <arg name="camera_id" value="0"/>
    <arg name="yolox_node_name" value="tensorrt_yolox$(var camera_id)"/>
    <arg name="image_transport_decompressor_node_name" value="image_transport_decompressor_node$(var camera_id)"/>
    <arg name="input/image" value="$(var image_raw0)"/>
    <arg name="output/objects" value="rois$(var camera_id)"/>
    <arg name="use_bytetrack" value="$(var use_bytetrack)"/>
  </include>
