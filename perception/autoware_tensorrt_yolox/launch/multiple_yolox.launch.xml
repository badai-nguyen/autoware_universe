<launch>
  <arg name="image_raw0" default="/sensing/camera/camera10/image_raw"/>
  <arg name="image_raw1" default="/image_raw1"/>
  <arg name="image_raw2" default="/image_raw2"/>
  <arg name="image_raw3" default="/image_raw3"/>
